{
	"name": "@micro-fleet/web",
	"version": "1.0.3",
	"description": "Contains classes used by web and rest services.",
	"scripts": {
		"build": "npm run compile && npm run test",
		"clean": "rimraf dist coverage .nyc_output",
		"definition": "node ../../../scripts/definition.js",
		"dedup": "rimraf node_modules/**/node_modules/**/typings",
		"compile": "npm run clean && npm run tslint && tsc --pretty",
		"release": "npm run build && npm run definition",
		"test": "nyc --reporter=json --reporter=html --include dist/app/**/*.js mocha --recursive dist/test",
		"tslint": "tslint -c tslint.json 'src/**/*.ts'",
		"prepublishOnly": "npm run release",
		"watch": "npm run clean && npm run tslint && tsc --watch --pretty"
	},
	"dependencies": {
		"body-parser": "~1.18",
		"cors": "~2.8",
		"express": "~4.15",
		"inversify-inject-decorators": "~3.0",
		"jsonwebtoken": "~8.3",
		"jwt-simple": "~0.5",
		"passport": "~0.4",
		"passport-jwt": "~4.0",
		"reflect-metadata": "~0.1",
		"winston": "~2.3"
	},
	"devDependencies": {
		"@types/body-parser": "~1.17",
		"@types/chai": "~4.1",
		"@types/chai-spies": "~0.0.1",
		"@types/cors": "~2.8",
		"@types/express": "~4.16",
		"@types/jsonwebtoken": "~7.2",
		"@types/joi": "~13.0",
		"@types/passport": "~0.4",
		"@types/passport-jwt": "~3.0",
		"@types/mocha": "~5.2",
		"@types/winston": "~2.3",
		"@types/node": "~8.10",
		"@types/request-promise": "~4.1",
		"joi": "~13.0",
		"chai": "~4.1",
		"chai-spies": "git://github.com/chaijs/chai-spies.git#b11aeebf",
		"request": "~2.83",
		"request-promise": "~4.2",
		"mocha": "~5.2",
		"nyc": "~12.0",
		"rimraf": "~2.6",
		"ts-declarator": "~1.0",
		"tslint": "~5.10",
		"typescript": "~2.9"
	},
	"peerDependencies": {
		"@micro-fleet/common": "~1.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"author": {
		"name": "gennovative",
		"url": "https://github.com/gennovative"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"license": "MIT",
	"private": false,
	"main": "./dist/app/index.js",
	"types": "./typings/app.d.ts",
	"repository": "git@github.com:gennovative/micro-fleet-web.git"
}
